{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "postUpgradeTasks": {
    "commands": ["cargo metadata"],
    "fileFilters": ["Cargo.toml", "**/*.js"],
    "executionMode": "update"
  },
  "enabledManagers": ["cargo"],
  "prConcurrentLimit": 5
}
